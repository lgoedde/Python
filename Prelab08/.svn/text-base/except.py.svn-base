if __name__ == "__main__":
    data = input("Please enter some values: ")
    data = data.split()
    newdata = []
    for item in data:
        try:
            item = float(item)
            newdata.append(item)
        except ValueError:
            pass
            
    print("The sum is: {}".format(sum(newdata)))

